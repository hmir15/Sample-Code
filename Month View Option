var timeStart = prompt("What time would you like it to start?");
var timeEnd = prompt("What time would you like it to end?");
//Seif's Code
        
function timeToPercent(time){
    
    var splitTime = time.split(":");
    var hours = parseInt(splitTime[0]);
    var minutes = parseInt(splitTime[1]);
    var totalMinutes = hours * 60 + minutes;
    
    return (totalMinutes / (12 * 60)) * (79.5-19.5) + 19.5;
}
    document.getElementById("testEvent").setAttribute("x1", timeToPercent(timeStart) + "%");
    
    document.getElementById("testEvent").setAttribute("x2", timeToPercent(timeEnd) + "%");
        
</script>

